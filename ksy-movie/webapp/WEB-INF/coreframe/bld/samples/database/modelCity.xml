<?xml version="1.0" encoding="euc-kr" ?>

<transaction name="modelCity" xmlns="http://www.cyber-i.com/xml/bld"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.cyber-i.com/xml/bld   http://www.cyber-i.com/xml/ns/bld-5.0.xsd">
	<info>
		<name></name>
		<description></description>
		<author></author>
		<version></version>
		<http-access>false</http-access>
	</info>

	<processor-info type="java">
	</processor-info>

	<input>
		<block>
			<field name="fieldNm" type="string" />
		</block>
	</input>

	<output>
		<block name="city">
			<field name="id" type="int" />
			<field name="name" type="string" />
			<field name="country" type="string" />
			<field name="airport" type="string" />
			<field name="language" type="string" />
			<field name="countryIsoCode" type="string" />
		</block>
	</output>

</transaction>
