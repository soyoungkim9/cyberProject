<?xml version="1.0" encoding="EUC-KR"?>
<transaction name="derby_suggest" xmlns="http://www.cyber-i.com/xml/bld"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.cyber-i.com/xml/bld   http://www.cyber-i.com/xml/ns/bld-5.0.xsd">
	<processor-info datasource="derbyPool" type="dbms">
		<transaction-type></transaction-type>		
		
		<sql id="query1">
			<query>
				SELECT CODE_NUM, CODE_NAME FROM CHART_TEST_SUGGEST				
			</query>
			<mapping>
				<setProperty></setProperty>
				<getProperty>code_num, code_name</getProperty>
			</mapping>
		</sql>
		
	</processor-info>
	<input>
		<block name="arg"></block>
	</input>
	<output>
		<block name="result">
			<field name="code_num" type="string"></field>
			<field name="code_name" type="string"></field>
		</block>
	</output>
</transaction>